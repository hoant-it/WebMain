{
  "name": "webmain",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --inspect ./bin/www --exec babel-node",
    "startDev": "concurrently \"npm run dev\" \"npm run watch\"",
    "build": "babel webmain -d build",
    "watch":"node-sass --watch resources/scss/ --output public/css/",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "crypto-js": "^4.1.1",
    "debug": "~2.6.9",
    "del": "^6.0.0",
    "dotenv": "^16.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-ejs-layouts": "^2.5.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mssql": "^8.0.2",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.5",
    "read-excel-file": "^5.2.28",
    "tedious": "^14.3.0",
    "xlsx": "^0.18.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "node-sass": "^7.0.1"
  }
}
